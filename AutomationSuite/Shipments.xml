<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="gapi">
	<listeners>
		<listener class-name="com.gapi.utilities.Reporting" />
	</listeners>
	<!--/-->
	<listeners>
		<listener class-name="com.gapi.testCases.AnnotationTransformer" />
	</listeners>
	<test name="Shipments">
		<classes>
			<class name="com.gapi.testCases.CreateShipmentsTicket" />
			<class name="com.gapi.testCases.GetShipmentsTicketDetails" />
			<class name="com.gapi.testCases.EditShippingReceivingTicket" />
			<class name="com.gapi.testCases.AddCommentToTheShipmentsTicket" />
			<class name="com.gapi.testCases.AddAttachmentToTheShipmentsTicket" />
			<class name="com.gapi.testCases.CancelShipmentsTicket" />
		</classes>
		
	</test> <!-- Test -->
</suite> <!-- Suite -->